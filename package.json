{
  "name": "canrocks",
  "version": "0.0.0",
  "description": "can.component rocks",
  "homepage": "",
  "author": {
    "name": "Matthew Phillips",
    "email": "matthew@matthewphillips.info",
    "url": ""
  },
  "scripts": {
    "test": "testee test.html --browsers firefox --reporter Spec --root http://localhost:8080",
    "start": "node lib/main",
    "can-serve": "can-serve --port 8080",
    "live-reload": "steal-tools live-reload",
    "develop": "bin/develop.js",
    "document": "documentjs"
  },
  "main": "canrocks/index.stache!done-autorender",
  "files": [
    "src"
  ],
  "keywords": [
    "canjs",
    "donejs"
  ],
  "donejs": {
    "deploy": {
      "root": "dist",
      "services": {
        "production": {
          "type": "s3",
          "bucket": "canrocks",
          "credentials": "../../.aws.s3.json"
        }
      }
    }
  },
  "system": {
    "main": "canrocks/index.stache!done-autorender",
    "directories": {
      "lib": "src"
    },
    "npmIgnore": [
      "documentjs",
      "testee",
      "donejs-deploy",
      "yeoman-environment",
      "generator-donejs",
      "npm",
      "lodash.some",
      "express",
      "compression",
      "xmlhttprequest",
      "body-parser",
      "pdenodeify",
      "get-package-readme",
      "cradle"
    ],
    "envs": {
      "production": {
        "baseURL": "http://canrocks.s3-website-us-east-1.amazonaws.com/dist/"
      }
    }
  },
  "canrocks": {
    "database": "components.db"
  },
  "dependencies": {
    "animate.css": "^3.4.0",
    "body-parser": "^1.13.3",
    "can": "^2.3.0-pre.8",
    "can-connect": "^0.2.7",
    "can-ssr": "^0.5.5",
    "compression": "^1.5.2",
    "cradle": "^0.6.9",
    "done-autorender": "^0.3.1",
    "done-component": "^0.2.0",
    "done-css": "^1.1.6",
    "express": "^4.13.3",
    "generator-donejs": "^0.2.9",
    "get-package-readme": "^1.1.0",
    "gravatarjs": "^1.0.0",
    "highlight.js": "^8.7.0",
    "jquery": "^2.1.4",
    "lodash.some": "^3.2.3",
    "lowdb": "^0.10.2",
    "marked": "^0.3.5",
    "npm": "^2.14.0",
    "pdenodeify": "^0.1.0",
    "steal": "^0.11.0-pre.6",
    "xmlhttprequest": "^1.7.0",
    "yeoman-environment": "^1.2.7",
    "can-simple-dom": "git://github.com/canjs/can-simple-dom#classname"
  },
  "devDependencies": {
    "documentjs": "^0.3.0",
    "donejs-deploy": "^0.1.5",
    "funcunit": "^3.0.0",
    "steal-qunit": "^0.1.1",
    "steal-tools": "^0.11.0-pre.9",
    "testee": "^0.2.0"
  }
}
